{{~#each output_variables}}
        {{type}} {{name}};{{/each}}
{{~/if}}
}struct_state;
 
{{#if transitions}}
/* helper functions */
void enter(state newStateLabel, struct_state *st);

void leave(state currentStateLabel, struct_state *st);{{/if}}
